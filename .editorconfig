# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 2

# data files for advent of code generally don't benefit from final newlines
[*.txt]
insert_final_newline = false
